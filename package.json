    "scripts": {
        "start": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; electron .",
        "web": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; electron . main-process-mode web",
        "build-all": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; BUILD_DATE=$(date -u +\"%Y-%m-%dT%H:%M:%SZ\") ; electron-builder -mwl",
        "build-mac": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; BUILD_DATE=$(date -u +\"%Y-%m-%dT%H:%M:%SZ\");  electron-builder --mac",
        "build-linux": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; BUILD_DATE=$(date -u +\"%Y-%m-%dT%H:%M:%SZ\");  electron-builder --linux",
        "build-windows": "ROOT=`pwd`; ./scripts/npm_start.sh $ROOT; BUILD_DATE=$(date -u +\"%Y-%m-%dT%H:%M:%SZ\");  electron-builder --windows",
        "build-doc": "./node_modules/.bin/typedoc --skipErrorChecking src --out doc/TypeDoc --entryPointStrategy expand",
        "dev": "webpack serve --mode development",
        "build": "webpack --mode production",
        "build-file-converter": "pkg ./dist/webpack/EdlFileConverter.js --targets node18-win-x64,node18-macos-x64,node18-linux-x64,node18-win-arm64,node18-macos-arm64,node18-linux-arm64 --out-path ./out"
    },
